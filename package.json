{
  "name": "portfolio-scola",
  "version": "1.0.0",
  "description": "personal portfolio website",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "nodemon server/index.js",
    "test": "jest",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "build:dev": "webpack --watch --mode=development --devtool=eval-source-map",
    "run:dev": "webpack serve --mode=development --devtool=eval-source-map",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-scola/portfolio-scola.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/j-scola/portfolio-scola/issues"
  },
  "homepage": "https://github.com/j-scola/portfolio-scola#readme",
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^17.0.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
